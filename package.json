{
  "name": "personal-website",
  "version": "1.0.0",
  "description": "Using Bulma and Jekyll",
  "main": "index.html",
  "dependencies": {},
  "devDependencies": {
    "bulma": "^0.8.2",
    "node-sass": "^4.13.1"
  },
  "scripts": {
    "build": "bundle exec jekyll build",
    "serve": "bundle exec jekyll serve --livereload",
    "css-build": "node-sass --omit-source-map-url _sass/main.scss assets/css/main_output.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch"
  },
